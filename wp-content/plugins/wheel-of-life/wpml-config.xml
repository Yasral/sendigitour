<wpml-config>
  <custom-types>
    <custom-type translate="1">wheel</custom-type>
    <custom-type translate="1">wheel-submissions</custom-type>
  </custom-types>
  <custom-fields>
    <custom-field action="translate">chartdata</custom-field>
    <custom-field action="translate">wheelid</custom-field>
    <custom-field action="translate">wheel_meta_input_type</custom-field>
    <custom-field action="translate">wheel_meta_range_min</custom-field>
    <custom-field action="translate">wheel_meta_range_max</custom-field>
    <custom-field action="translate">wheel_meta_wheels_data</custom-field>
